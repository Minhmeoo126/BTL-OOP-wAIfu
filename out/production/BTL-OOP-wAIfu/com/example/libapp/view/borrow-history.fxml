<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.libapp.controllers.BorrowingHistoryController">
    <TableView fx:id="historyTable">
        <columns>
            <TableColumn fx:id="bookIdColumn" text="Book ID" />
            <TableColumn fx:id="borrowDateColumn" text="Borrow Date" />
            <TableColumn fx:id="returnDateColumn" text="Return Date" />
        </columns>
    </TableView>
    <Button fx:id="backToMain" onAction="#backToMain" text="Back to Main" />
</VBox>
